{
  "name": "@coinbase/cdp-react",
  "version": "0.0.75",
  "type": "module",
  "dependencies": {
    "@internationalized/number": "3.6.4",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-form": "^0.1.7",
    "@radix-ui/react-one-time-password-field": "^0.1.7",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-select": "^2.2.6",
    "big.js": "7.0.1",
    "clsx": "^2.1.1",
    "libphonenumber-js": "^1.12.10"
  },
  "peerDependencies": {
    "react": ">=18.2.0 <19.2.0",
    "@coinbase/cdp-core": "^0.0.75",
    "@coinbase/cdp-hooks": "^0.0.75"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^11.2.0",
    "@size-limit/webpack": "^11.2.0",
    "@size-limit/webpack-css": "^11.2.0",
    "@size-limit/webpack-why": "^11.2.0",
    "@testing-library/dom": "^10.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/big.js": "6.2.2",
    "@types/glob": "^9.0.0",
    "@types/react": "~19.1.0",
    "@types/react-dom": "~19.1.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.3",
    "jest-canvas-mock": "2.5.2",
    "react": "~19.1.2",
    "react-dom": "~19.1.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "size-limit": "^11.2.0",
    "stylelint": "^16.22.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "@coinbase/cdp-core": "^0.0.75",
    "@coinbase/cdp-hooks": "^0.0.75"
  },
  "size-limit": [
    {
      "name": "full-package",
      "path": "./dist/index.js",
      "import": "*",
      "running": false,
      "limit": "115 KB"
    },
    {
      "name": "single-component",
      "path": "./dist/components/SignIn/index.js",
      "import": "{ SignIn }",
      "running": false,
      "limit": "89 KB"
    }
  ],
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./components/*": {
      "import": {
        "types": "./dist/components/*/index.d.ts",
        "default": "./dist/components/*/index.js"
      }
    },
    "./components/forms/*": {
      "import": {
        "types": "./dist/components/forms/*/index.d.ts",
        "default": "./dist/components/forms/*/index.js"
      }
    },
    "./components/ui/*": {
      "import": {
        "types": "./dist/components/ui/*/index.d.ts",
        "default": "./dist/components/ui/*/index.js"
      }
    },
    "./icons": {
      "import": {
        "types": "./dist/icons/index.d.ts",
        "default": "./dist/icons/index.js"
      }
    },
    "./icons/*": {
      "import": {
        "types": "./dist/icons/*.d.ts",
        "default": "./dist/icons/*.js"
      }
    },
    "./theme": {
      "import": {
        "types": "./dist/theme/index.d.ts",
        "default": "./dist/theme/index.js"
      }
    },
    "./theme/*": {
      "import": {
        "types": "./dist/theme/*.d.ts",
        "default": "./dist/theme/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "pnpm run check:types && vite build",
    "build:watch": "vite build --watch",
    "analyze": "vite build --config vite-analyze.config.ts",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:all": "pnpm clean && rm -rf node_modules",
    "size-limit": "size-limit",
    "test": "vitest",
    "test:run": "vitest --run",
    "genc": "node ../../scripts/generate-component.mjs src/components"
  }
}